<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--        <item name="android:windowBackground">@color/colorPrimaryDark</item>-->
        <item name="android:windowBackground">@android:color/black</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:ambientShadowAlpha">0.007</item>
        <item name="android:spotShadowAlpha">0.07</item>
        <item name="android:textColorPrimary">@color/main_category_text_color</item>
        <item name="colorOnSurface">?android:textColorPrimary</item>
        <item name="snackbarStyle">@style/MySnackbar</item>
        <item name="android:layoutDirection">ltr</item>
    </style>

    <style name="SwitchTextAppearance" parent="@android:style/TextAppearance.Holo.Small">
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="tallerBarStyle" parent="@android:style/Widget.SeekBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@color/bluegrey</item>
        <item name="android:indeterminateDrawable">@color/bluegrey</item>
        <item name="android:minHeight">8dip</item>
        <item name="android:maxHeight">10dip</item>
    </style>

    <style name="MySnackbar" parent="Widget.MaterialComponents.Snackbar">
        <item name="android:layout_margin">@dimen/_10sdp</item>
        <item name="android:translationY">-90dp</item>
        <item name="android:textColor">@color/main_category_text_color</item>
    </style>

    <style name="AppTheme.CenterAnimation">
        <!--        <item name="android:windowBackground">@drawable/ic_search</item>-->
    </style>

    <style name="HintText" parent="TextAppearance.Design.Hint">
        <item name="android:textSize">@dimen/_14sdp</item>
        <item name="android:textColor">@color/textSmall</item>
    </style>

    <style name="FullScreenDialogStyle" parent="StatusBarWhite">
        <item name="android:windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorPrimary">@color/colorPrimary</item>

        <!-- Set this to true if you want Full Screen without status bar -->
        <item name="android:windowFullscreen">false</item>

        <item name="android:windowIsFloating">false</item>

        <item name="android:windowContentOverlay">@null</item>

        <!-- This is important! Don't forget to set window background -->
        <item name="android:windowBackground">@color/transparentBlack</item>

        <item name="android:ambientShadowAlpha">0.005</item>
        <item name="android:spotShadowAlpha">0.05</item>

    </style>

    <!--WHITE STATUS BAR (in v23 will be transparent)-->
    <style name="StatusBarWhite" parent="Theme.AppCompat.NoActionBar">
        <item name="android:statusBarColor">@color/black</item>
    </style>

    <style name="AlertDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="colorAccent">@color/black</item>
    </style>

    <style name="SportsTab" parent="Widget.Design.TabLayout">
